
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Usuario {
  id         Int     @id  @default(autoincrement())
  nome       String
  sobrenome  String
  idade      Int
  cpf       String  @unique
  email      String  @unique
  telefone   BigInt?
}

model Medico {
  id         Int      @id  @default(autoincrement())
  nome       String
  sobrenome  String
  idade      Int
  CPF        String   @unique
  email      String   @unique
  CRM        String   @unique
  telefone   BigInt
}

model Gestor {
  codigo     Int      @id  @default(autoincrement())
  nome       String
  sobrenome  String
}